
class Solution {
  public:
    // Function to count nodes of a linked list.
    int getCount(struct Node* head) {

        // Code here
        if(head==NULL) return 0;
        
        Node* p=head;
        int cnt=0;
        while(p!=NULL){
            cnt++;
            p=p->next;
        }
        return cnt;
    }
};
